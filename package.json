    "add": "git add -A",
    "commit": "git add -A && git commit -m \"ready for deploy\"",
    "push:heroku": "git push heroku main",
    "deploy": "npm run client:build && npm run add && npm run commit && npm run push:heroku",
    "start": "cd ./server && npm start",
    "start:testwin" : "cd ./server && set ",
    "start:testmac": ""
